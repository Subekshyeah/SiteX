# Auto detect text files and perform LF normalization
* text=auto

# Jupyter notebooks should always use LF
*.ipynb text eol=lf

# Python files
*.py text eol=lf

# Web files
*.js text eol=lf
*.ts text eol=lf
*.tsx text eol=lf
*.jsx text eol=lf
*.css text eol=lf
*.html text eol=lf
*.json text eol=lf
*.md text eol=lf

# Data files
*.csv text eol=lf
*.txt text eol=lf
*.geojson text eol=lf

# Shell scripts
*.sh text eol=lf
